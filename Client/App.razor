<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(StandardLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(LoginLayout)">
            <Result Title="404" SubTitle="The resource does not exists" Status="404"></Result>
        </LayoutView>
    </NotFound>
</Router>
<AntContainer />
